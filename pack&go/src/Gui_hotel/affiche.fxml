<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane id="AnchorPane" prefHeight="616.0" prefWidth="754.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="Gui_hotel.AfficheController">
   <children>
      <TableView fx:id="table" layoutX="24.0" layoutY="24.0" prefHeight="368.0" prefWidth="724.0">
         <columns>
            <TableColumn fx:id="idH" prefWidth="75.0" text="Hotel Id" />
            <TableColumn fx:id="nomH" prefWidth="75.0" text="Hotel Name" />
            <TableColumn fx:id="categoryH" prefWidth="75.0" text="Hotel Category" />
            <TableColumn fx:id="adressH" prefWidth="75.0" text="Hotel Adress" />
            <TableColumn fx:id="email" prefWidth="75.0" text="E-Mail" />
            <TableColumn fx:id="tel" prefWidth="75.0" text="Phone Number" />
            <TableColumn fx:id="equipment" prefWidth="75.0" text="Equipments" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="add" layoutX="86.0" layoutY="473.0" mnemonicParsing="false" onAction="#back_add" text="add hotel" />
      <Button fx:id="delete" layoutX="188.0" layoutY="473.0" mnemonicParsing="false" onAction="#delete" text="delete hotel" />
   </children>
</AnchorPane>
